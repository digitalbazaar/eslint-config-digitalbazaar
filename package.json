{
  "name": "eslint-config-digitalbazaar",
  "version": "5.2.1-0",
  "description": "Digital Bazaar's eslint rules",
  "type": "module",
  "main": "./index.js",
  "exports": {
    ".": "./index.js",
    "./jsdoc": "./jsdoc.js",
    "./module": "./module.js",
    "./vue": "./vue.js",
    "./vue3": "./vue3.js"
  },
  "files": [
    "import.js",
    "index.js",
    "jsdoc.js",
    "module.js",
    "vue-common.js",
    "vue.js",
    "vue3.js",
    "templates/**/*.js"
  ],
  "scripts": {
    "test": "eslint",
    "lint": "eslint --ignore-pattern 'test/**/*.js'"
